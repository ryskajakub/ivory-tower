{
  "name": "ivory-tower",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "babel": "node_modules/.bin/babel --extensions '.ts' --source-maps --presets @babel/preset-typescript --out-dir dist src",
    "example": "node --enable-source-maps --es-module-specifier-resolution=node dist/example/runServer.js",
    "schema": "node --enable-source-maps --es-module-specifier-resolution=node dist/example/runSchema.js > schema.prisma",
    "migrate": "node_modules/.bin/prisma migrate dev --schema=./schema.prisma"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryskajakub/ivory-tower.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ryskajakub/ivory-tower/issues"
  },
  "homepage": "https://github.com/ryskajakub/ivory-tower#readme",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/plugin-transform-typescript": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^18.11.9",
    "nodemon": "^2.0.20",
    "prisma": "^4.6.1",
    "typescript": "^4.9.3"
  },
  "nodemonConfig": {
    "watch": [
      "src/"
    ],
    "ext": "ts"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "pg-promise": "^10.15.0"
  }
